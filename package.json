{
  "name": "phaser-font-plugin",
  "type": "module",
  "version": "0.1.1",
  "description": "Phaser 3 plugin to load fonts",
  "author": {
    "name": "hywax",
    "url": "https://hywax.space/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hywax/phaser-font-plugin.git"
  },
  "keywords": [
    "phaser",
    "phaser-plugin",
    "font",
    "loader"
  ],
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "pnpm lint && pnpm build && changelogen --release --push && pnpm publish"
  },
  "peerDependencies": {
    "phaser": "^3.80.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.0",
    "changelogen": "^0.5.5",
    "eslint": "^9.4.0",
    "phaser": "^3.80.1",
    "rollup": "^4.18.0",
    "unbuild": "^2.0.0"
  }
}
